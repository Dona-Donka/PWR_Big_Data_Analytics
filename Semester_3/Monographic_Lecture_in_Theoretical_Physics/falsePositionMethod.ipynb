   ],
   "source": [
    "from math import exp, cos\n",
    "\n",
    "accuracy = 10**(-5)\n",
    "a = 8\n",
    "b = 12\n",
    "c= 0\n",
    "\n",
    "while accuracy < abs(function(c)):\n",
    "    c = a - (b-a)*function(a)/(function(b) - function(a))\n",
    "    if function(a) * function(c) < 0:\n",
    "        b = c\n",
    "    else:\n",
    "        a = c\n",
    "print(c)"
   ]
